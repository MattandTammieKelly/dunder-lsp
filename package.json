{
  "name": "dunder-lsp",
  "scripts": {
    "gen-protos": "pbjs -t static-module -w commonjs -o src/proto.js proto/rpc.proto proto/router.proto && yarn pbts -o src/proto.d.ts src/proto.js",
    "build": "tsc -p tsconfig.json",
    "start": "yarn build && node dist/index.js",
    "watch": "concurrently \"tsc -p tsconfig.json -w\" \"nodemon -w dist dist/index.js\"",
    "gen-proto": "  pbjs --force-long -t static-module -w commonjs -o src/proto.js proto/rpc.proto proto/router.proto && pbts -o src/proto.d.ts src/proto.js"
  },
  "version": "1.0.0",
  "main": "index.js",
  "author": "Hampus Sj√∂berg",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "1.2.4",
    "@grpc/proto-loader": "0.5.6",
    "@types/config": "^0.0.38",
    "bech32": "^1.1.4",
    "concurrently": "^5.3.0",
    "config": "^3.3.3",
    "fastify": "3.10.1",
    "fastify-cors": "5.1.0",
    "fastify-websocket": "2.1.0",
    "grpc": "1.24.4",
    "nodemon": "^2.0.7",
    "protobufjs": "6.10.2",
    "typescript": "4.1.3"
  },
  "devDependencies": {
    "@types/node": "14.14.22"
  }
}
